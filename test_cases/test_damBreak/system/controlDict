FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     foamRun;
solver          incompressibleCVoF;

startFrom       startTime;
startTime       0;

stopAt          endTime;
endTime         1;

deltaT          0.001;

writeControl    adjustableRunTime;
writeInterval   0.05;

purgeWrite      0;
writeFormat     ascii;
writePrecision  6;
writeCompression off;

timeFormat      general;
timePrecision   6;

runTimeModifiable yes;
adjustTimeStep  no;

maxCo           0.8;
maxAlphaCo      0.8;

maxDeltaT       1;

