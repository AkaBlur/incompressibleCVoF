FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      C;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 1e-9;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    wall
    {
        type            slip;
    }

    frontAndBack
    {
        type            empty;
    }
}


